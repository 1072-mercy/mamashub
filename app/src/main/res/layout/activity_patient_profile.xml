                                        android:layout_height="wrap_content"
                                        android:src="@drawable/pregnant" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>

                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="Previous Pregnancy"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/navigateAntenatal"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                android:layout_weight="1"
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewAntenatal"                            android:layout_width="90dp"
                                android:layout_height="90dp">


                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/antenatalProfile"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/dashboard_maternity_icon_label"
                                        android:src="@drawable/antenatalprofile" />

                                </LinearLayout>


                            </androidx.cardview.widget.CardView>


                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="@string/antenatal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/navigateBirthPlan"
                            android:layout_marginEnd="10dp"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">


                            <androidx.cardview.widget.CardView
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewBirthPlan"
                                android:layout_width="90dp"
                                android:layout_height="90dp">

                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/birthPlan"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/dashboard_maternity_icon_label"
                                        android:src="@drawable/birth_plan" />

                                </LinearLayout>




                        </androidx.cardview.widget.CardView>

                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="@string/birth_plan"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>


                    </TableRow>

                    <TableRow
                        android:layout_marginTop="@dimen/_15dp">

                        <LinearLayout
                            android:id="@+id/navigateClinicalNotes"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                android:layout_weight="1"
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewClinicalNotes"
                                android:layout_width="90dp"
                                android:layout_height="90dp">

                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/clinicalNotes"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/clinical_notes" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>

                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="@string/clinical_notes"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/navigatePresent"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                android:layout_weight="1"
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewPresentPregnancy"                            android:layout_width="90dp"
                                android:layout_height="90dp">


                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/presentPregnancy"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/dashboard_maternity_icon_label"
                                        android:src="@drawable/present_pregnancy" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>


                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="@string/present_pregnancy"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/navigatePreventiveService"
                            android:layout_marginEnd="10dp"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">


                            <androidx.cardview.widget.CardView
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewPreventiveServices"
                                android:layout_width="90dp"
                                android:layout_height="90dp">

                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/preventiveServices"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/dashboard_maternity_icon_label"
                                        android:src="@drawable/tetanusinjection" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>

                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="@string/preventive_services"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>


                    </TableRow>

                    <TableRow
                        android:layout_marginTop="@dimen/_15dp">

                        <LinearLayout
