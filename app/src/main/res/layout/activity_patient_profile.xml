                                android:layout_height="90dp">

                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/malariaProphylaxis"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/malaria_prophylaxis" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>

                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="@string/malaria_prophylaxis"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/navigateMaternalSerology"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                android:layout_weight="1"
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewMaternalSerology"                            android:layout_width="90dp"
                                android:layout_height="90dp">


                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/maternalSerology"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/dashboard_maternity_icon_label"
                                        android:src="@drawable/maternal_serology" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>


                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="Maternal Serology"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/navigateWeight"
                            android:layout_marginEnd="10dp"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">


                            <androidx.cardview.widget.CardView
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewWeightChart"
                                android:layout_width="90dp"
                                android:layout_height="90dp">

                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/weightChart"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/dashboard_maternity_icon_label"
                                        android:src="@drawable/weight_monitoring" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>

                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="Weight Monitoring Chart"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>


                    </TableRow>


                    <TableRow
                        android:layout_marginTop="@dimen/_15dp">

                        <LinearLayout
                            android:id="@+id/navigateIfas"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                android:layout_weight="1"
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewIfas"                            android:layout_width="90dp"
                                android:layout_height="90dp">

                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

