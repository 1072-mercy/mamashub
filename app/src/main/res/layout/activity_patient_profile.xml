<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_margin="10dp"
    tools:context=".new_designs.screens.PatientProfile">


    <LinearLayout
        android:background="@drawable/layout_dotted"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:layout_width="@dimen/_100dp"
            android:layout_height="@dimen/_100dp">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageViewProfile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/profile"
                app:civ_border_color="#FF000000"
                app:civ_border_width="0dp" />


        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TableLayout
                android:layout_marginStart="10dp"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TableRow>

                    <TextView
                        android:textStyle="bold"
                        android:textColor="@color/textColor"
                        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                        android:text="Patient Name  "
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>

                    <TextView
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/textColor"
                        android:text=":"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                    <TextView
                        android:id="@+id/tvName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Jane Doe"
                        android:textColor="@color/textColor" />

                </TableRow>
                <TableRow
                    android:visibility="gone"
                    >

                    <TextView
                        android:visibility="gone"
                        android:textStyle="bold"
                        android:textColor="@color/textColor"
                        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                        android:text="ANC ID  "
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>

                    <TextView
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/textColor"
                        android:text=":"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/textColor" />

                </TableRow>
                <TableRow>

                    <TextView
                        android:textStyle="bold"
                        android:textColor="@color/textColor"
                        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                        android:text="Age"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>

                    <TextView
                        android:layout_marginEnd="10dp"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/textColor"
                        android:text=":"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                    <TextView
                        android:id="@+id/tvAge"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Jane Doe"
                        android:textColor="@color/textColor" />

                </TableRow>



            </TableLayout>

            <View
                android:layout_margin="@dimen/_5dp"
                android:background="@color/primary"
                android:layout_width="match_parent"
                android:layout_height="1dp"/>

            <LinearLayout
                android:layout_marginStart="10dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:textStyle="bold"
                    android:textColor="@color/textColor"
                    android:textAppearance="@style/TextAppearance.AppCompat.Medium"
                    android:text="Next of Kin Details:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/tvKinName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:drawablePadding="10dp"
                        android:text="Patient Name"
                        android:textColor="@color/textColor"/>


                    <TextView
                        android:id="@+id/tvKinDetails"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:drawablePadding="10dp"
                        android:text="Patient Number"
                        android:textAppearance="@style/TextAppearance.AppCompat.Small"
                        android:textColor="@color/textColor" />


                </LinearLayout>



            </LinearLayout>

        </LinearLayout>




    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
            android:textStyle="bold"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:layout_marginBottom="@dimen/_15dp"
            android:textColor="@color/headerColor"
            android:text="Patient Information"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TableRow>

                        <LinearLayout
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                android:layout_weight="1"
                                app:cardCornerRadius="10dp"
                                android:id="@+id/findPatientsCardView"                            android:layout_width="90dp"
                                android:layout_height="90dp">

                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/imageViewEditPatient"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:src="@drawable/register" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>

                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="@string/edit_patient"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/navigateMedicalHistory"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <androidx.cardview.widget.CardView
                                android:layout_weight="1"
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardviewHistory"
                                android:layout_width="90dp"
                                android:layout_height="90dp">


                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
                                        android:paddingBottom="@dimen/_5dp"
                                        android:id="@+id/medicalHistory"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:contentDescription="@string/dashboard_maternity_icon_label"
                                        android:src="@drawable/surgery" />

                                </LinearLayout>




                            </androidx.cardview.widget.CardView>


                            <TextView
                                android:textColor="@color/headerColor"
                                android:layout_marginTop="@dimen/_5dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:text="@string/action_medical"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"/>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/navigatePhysicalExam"
                            android:layout_marginEnd="10dp"
                            android:gravity="center"
                            android:layout_gravity="center"
                            android:layout_weight="1"
                            android:layout_marginStart="10dp"
                            android:orientation="vertical"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">


                            <androidx.cardview.widget.CardView
                                app:cardCornerRadius="10dp"
                                android:id="@+id/cardViewPhysicalExam"
                                android:layout_width="90dp"
                                android:layout_height="90dp">

                                <LinearLayout
                                    android:background="@color/accent"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent">

                                    <ImageView
